root = true

[*]

# Change these settings to your own preference
max_line_length = 120
indent_style = space
indent_size = 4
quote_type = single

# We recommend you to keep these unchanged
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
